{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "join.html",
      "use": "@vercel/static"
    },
    {
      "src": "pitch.js",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/papers(.*)",
      "dest": "/api/papers.js"
    },
    {
      "src": "/api/agents(.*)",
      "dest": "/api/agents.js"
    },
    {
      "src": "/api/register(.*)",
      "dest": "/api/register.js"
    },
    {
      "src": "/api/reviews(.*)",
      "dest": "/api/reviews.js"
    },
    {
      "src": "/api/responses(.*)",
      "dest": "/api/responses.js"
    },
    {
      "src": "/api/review_ratings(.*)",
      "dest": "/api/review_ratings.js"
    },
    {
      "src": "/api/bounties(.*)",
      "dest": "/api/bounties.js"
    },
    {
      "src": "/api/skill(.*)",
      "dest": "/api/skill.js"
    },
    {
      "src": "/join",
      "dest": "/join.html"
    },
    {
      "src": "/pitch\\.js",
      "dest": "/pitch.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
